{
  "manifest_version": 3,
  "name": "Next.js Extension",
  "version": "1.0",
  "description": "A simple browser extension using Next.js",
  "permissions": ["scripting", "tabs"],
  "action": {
    "default_popup": "popup.html"
  },
  "icons": {
    "16": "icon.png",
    "48": "icon.png",
    "128": "icon.png"
  }
}